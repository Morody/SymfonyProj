<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}AdminPanel{% endblock %}</title>
    <link rel="stylesheet" href=" {{ asset('css/style.css')}} ">
    {% block script %}
    {% endblock %}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<header>
    {% block header %}
        <div class="header_logo">Vi</div>
            <button class="logout"><a href="{{ path('app_logout') }}">Выйти</a></button>
        {% else %}
            <button class="singin"><a href="{{ path('app_login') }}">Войти</a></button>
            <button class="register"><a href="{{ path('app_register') }}">Регистрация</a></button>
        {% endif %}
    {% endblock %}
</header>

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    AdminPanel

</div>
{% endblock %}
